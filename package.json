{
  "name": "local-crm",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "concurrently -k \"npm:dev --workspace apps/api\" \"npm:dev --workspace apps/web\"",
    "db:seed": "npm run db:seed --workspace apps/api",
    "build": "npm run build --workspace apps/web",
    "lint": "echo 'no lint'"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
